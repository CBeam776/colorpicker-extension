{
  "manifest_version": 2,

  "name": "background-colorpeek",
  "version": "1.0",
  "description": "Get background color from current tab.",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },

  "permissions": ["<all_urls>"]

}
